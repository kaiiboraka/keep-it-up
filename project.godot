; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Keep It Up"
run/main_scene="res://_Assets/game.tscn"
config/features=PackedStringArray("4.4", "C#", "Forward Plus")
config/icon="res://icon.svg"

[aseprite]

animation/layers/only_include_visible_layers_by_default=true

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/stretch/aspect="keep_width"
window/stretch/scale_mode="integer"

[dotnet]

project/assembly_name="Keep It Up"

[editor_plugins]

enabled=PackedStringArray("res://addons/AsepriteWizard/plugin.cfg", "res://addons/DragNDropNodes/plugin.cfg", "res://addons/Todo_Manager/plugin.cfg")

[file_customization]

folder_colors={
"res://_Assets/Buildings/": "teal",
"res://_Assets/Characters/": "blue",
"res://_Assets/Characters/Archer/": "green",
"res://_Assets/Characters/Flying/": "blue",
"res://_Assets/Characters/Magic/": "purple",
"res://_Assets/Characters/Melee/": "red",
"res://_Assets/Level/": "purple",
"res://_Assets/UI/": "pink"
}

[importer_defaults]

texture={
"compress/channel_pack": 0,
"compress/hdr_compression": 1,
"compress/high_quality": false,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
"detect_3d/compress_to": 1,
"mipmaps/generate": false,
"mipmaps/limit": -1,
"process/fix_alpha_border": true,
"process/hdr_as_srgb": false,
"process/hdr_clamp_exposure": false,
"process/normal_map_invert_y": false,
"process/premult_alpha": false,
"process/size_limit": 0,
"roughness/mode": 0,
"roughness/src_normal": ""
}

[layer_names]

2d_physics/layer_1="Ground"
2d_physics/layer_2="Buildings"
2d_physics/layer_3="Player_Hurtbox"
2d_physics/layer_4="Enemy_Hurtbox"
2d_physics/layer_5="Walkthrough"
2d_physics/layer_6="Impassable"
2d_physics/layer_7="Player_Attacks"
2d_physics/layer_8="Enemy_Attacks"
2d_physics/layer_9="Player_UnitBodies"
2d_physics/layer_10="Enemy_UnitBodies"

[rendering]

textures/canvas_textures/default_texture_filter=0
